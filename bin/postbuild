#!/usr/bin/env node

/**
 * Module dependencies.
 */
var shell = require('shelljs');
var chalk = require('chalk');

/**
 * Shell Executions code
 */
shell.echo(chalk.blue('Moving the content from temp folder in dist folder'));
shell.mv('temp/*', ' dist');

shell.echo(chalk.blue('Deleting temp folder'));
shell.rm('-rf', 'temp');
